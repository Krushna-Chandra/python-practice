num = int(input("Enter a number:"))
if (num<=1):
    print("Its not a prime number!")
elif(num==2):
    print("Its a prime number!")
else:
    for i in range(2,num):
        if(num%i==0):
            print("Its not a prime number!")
            break
        else:
            print("Its a prime number!")
            break
    





n = int(input("Enter a number: "))
rev = 0
while n>0:
    rev = rev * 10 + n % 10
    n //= 10
print(f"The reverse of the number is",rev)





